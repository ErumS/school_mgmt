<h1>Teachers:</h1><br>
<table>
  <tr>
    <th>Name</th>
  </tr>
  <% @teachers.each do |teacher| %>
    <tr>
      <td><%= teacher.name %></td>
      <td><%= link_to 'Show', teacher_path(teacher) %></td>
      <td><%= link_to 'Edit teacher', edit_teacher_path(teacher) %></td>  
      <td><%= link_to 'Destroy', teacher_path(teacher), method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
</table>
<br>
<%= link_to 'Home', schools_path %>